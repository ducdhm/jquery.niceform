{
    "ignore": [
        "node_modules/**/*"
    ],
    "presets": [
        [
            "@babel/env",
            {
                "modules": "umd"
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-modules-umd",
            {
                "globals": {
                    "jquery": "jQuery"
                }
            }
        ],
        "@babel/plugin-proposal-class-properties"
    ]
}